{
  "name": "movie-platform",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run build",
    "dev": "npm-run-all --parallel dev:sass dev:react",
    "dev:react": "webpack --watch",
    "dev:sass": "node-sass --watch src/css/main.scss build/css/style.css",
    "start": "node server.js",
    "build": "npm-run-all --parallel build:sass build:react",
    "build:react": "webpack -p",
    "build:sass": "node-sass src/css/main.scss build/css/style.css",
    "test": "jest --verbose",
    "migrate": "npm-run-all -s migrate-down migrate-up",
    "migrate-down": "db-migrate reset --config ./database/config/database.json --migrations-dir ./database/config/migrations",
    "migrate-up": "db-migrate up --config ./database/config/database.json --migrations-dir ./database/config/migrations",
    "lint": "npm-run-all -s lint-front lint-routes lint-models",
    "lint-front": "eslint src",
    "lint-back": "npm-run-all -s lint-routes lint-models",
    "lint-routes": "eslint routes",
    "lint-models": "eslint models",
    "supertest": "node server_tests/index_test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hedvegia/movie-app.git"
  },
  "dependencies": {
    "antd": "^3.1.1",
    "aws-sdk": "^2.83.0",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "db-migrate": "^0.10.2",
    "db-migrate-mysql": "^1.1.10",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "express": "^4.15.3",
    "history": "^4.6.3",
    "install": "^0.10.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-enzyme": "^4.0.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "mysql": "^2.15.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "pg": "6.4.0",
    "postinstall-build": "^5.0.1",
    "prop-types": "^15.6.0",
    "query-string": "^4.3.4",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-player": "^0.19.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^15.6.1",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "supertest": "^3.0.0",
    "sweetalert": "^2.1.0",
    "tape": "^4.6.3",
    "webpack": "^3.2.0",
    "xit": "^0.2.0"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015",
      "babel-preset-react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\css.": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^0.28.8",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "style-loader": "^0.19.1",
    "supertest": "^3.0.0",
    "tape": "^4.8.0"
  }
}
